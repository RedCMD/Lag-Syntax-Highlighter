{
	"$schema": "https://raw.githubusercontent.com/RedCMD/TmLanguage-Syntax-Highlighter/main/vscode.tmLanguage.schema.json",
	"name": "lag",
	"scopeName": "source.lag",
	"abc": "def",
	"patterns": [
		{
			"match": "\\b(true|false)\\b",
			"captures": { "0": { "name": "comment" } }
		},
		{
			"match": "\\b(true|false)\\b",
			"captures": [ { "name": "comment" } ]
		},
		{
			"match": "\\b(true|false)\\b",
			"captures": {
				"comment": "string",
				"0": { "name": "comment" }
			}
		},
		{
			"match": "\\b(true|false)\\b",
			"captures": {
				"comment": [ "array" ],
				"0": { "name": "comment" }
			}
		},
		{
			"match": "\\b(true|false)\\b",
			"captures": {
				"comment": { "object": "string" },
				"0": { "name": "comment" }
			}
		},
		{
			"match": "\\b(true|false)\\b",
			"captures": {
				"//": "string",
				"0": { "name": "comment" }
			}
		},
		{
			"foo": "bar"
		}
	],
	"repository": { }
}
